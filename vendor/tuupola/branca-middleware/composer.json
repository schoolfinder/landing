{
    "name": "tuupola/branca-middleware",
    "description": "PSR-7 and PSR-15 Branca Authentication Middleware",
    "keywords": [
        "psr-7",
        "psr-15",
        "middleware",
        "jwt",
        "branca",
        "auth"
    ],
    "homepage": "https://github.com/tuupola/branca-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Mika Tuupola",
            "email": "tuupola@appelsiini.net",
            "homepage": "http://www.appelsiini.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.6 || ^7.0",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0",
        "tuupola/branca": "^0.3.0",
        "http-interop/http-middleware": "^0.4.1",
        "http-interop/http-factory": "^0.3.0",
        "tuupola/http-factory": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "overtrue/phplint": "^0.2.4",
        "zendframework/zend-diactoros": "^1.3",
        "equip/dispatch": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Tuupola\\Middleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tuupola\\Middleware\\": "tests"
        }
    }
}
